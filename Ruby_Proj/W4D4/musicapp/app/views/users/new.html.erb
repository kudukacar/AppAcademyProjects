<h1> Sign up! <h1>
<form action="<%=users_url%>" method="POST">
<%=auth_token%>

    <label for="email">Email</label>
    <input
        type="text"
        id="email"
        name="user[email]"
        value="<%=@user.email%>">

    <label for="password">Password</label>
    <input
        type="password"
        id="password"
        name="user[password]"
        value="">

    <input type="submit" value="Sign up!">
</form>