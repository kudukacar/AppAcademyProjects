<h1>Login<h1>
<form action="<%=session_url%>" method="POST">

<%=auth_token%>

<label for="email">Email</label>
<input
    type="text"
    id="email"
    name="user[email]"
    value="">

<label for="password">Password</label>
<input
    type="password"
    id="password"
    name="user[password]"
    value=""> 

<input type="submit" value="Login" >
</form>
